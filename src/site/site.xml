<?xml version="1.0" encoding="UTF-8"?>

<project name="Kitchenoffice Project" xmlns="http://maven.apache.org/DECORATION/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
	<bannerLeft>
		<name>Kitchenoffice Project Documentation</name>
	</bannerLeft>

	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.3.0</version>
	</skin>
	<custom>
		<fluidoSkin>
			<topBarEnabled>true</topBarEnabled>
			<sideBarEnabled>true</sideBarEnabled>
			<googleSearch>
				<sitesearch />
			</googleSearch>
			<gitHub>
				<projectId>gentics/kitchenoffice</projectId>
				<ribbonOrientation>right</ribbonOrientation>
				<ribbonColor>black</ribbonColor>
			</gitHub>
		</fluidoSkin>
	</custom>


	<body>
		<links>
			<item name="Gentics Website" href="http://www.gentics.com" />
		</links>

		<menu name="Project Documentation">
			<item name="Welcome" href="index.html" />
			<item name="Technologies" href="technologies.html" />
		</menu>
		
		<!-- Inherit this menu for sub modules. -->
		<menu ref="parent" inherit="top" />
		
		<menu name="Project Modules" ref="modules" />

		<menu ref="reports" name="Reports" inherit="bottom" />
	</body>
</project>
